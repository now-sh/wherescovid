{
  "name": "location-covid-tracker",
  "version": "2.0.0",
  "description": "Location-based COVID-19 tracker with Vue 3 and Dracula theme",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --fix",
    "serve": "node server.js",
    "deploy": "gitcommit deploy && npm run build && vercel && vercel --prod"
  },
  "keywords": ["covid19", "location", "tracker", "geolocation", "vue3", "dracula-theme"],
