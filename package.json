  "license": "MIT",
  "dependencies": {
    "vue": "^3.5.13",
    "bootstrap": "^5.3.3",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.1",
    "vite": "^7.1.4",
    "vitest": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "jsdom": "^26.1.0",
    "eslint": "^9.35.0",
    "globals": "^15.14.0",
    "@eslint/js": "^9.16.0",
    "eslint-plugin-vue": "^10.4.0",
    "eslint-config-prettier": "^9.1.0"
  }
