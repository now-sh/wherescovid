{
  "name": "wherescovid",
  "version": "1.0.1",
  "description": "Covid-19 cases",
  "main": "src/index.html",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "NODE_ENV=production parcel build src/index.html --no-source-maps --out-dir public",
    "dev": "NODE_ENV=development parcel build src/index.html --no-source-maps --out-dir dist && lite-server --baseDir=dist",
    "deploy": "gitcommit deploy;NODE_ENV=development parcel build src/index.html --no-source-maps --out-dir public && vercel deploy --prod"
  },
  "keywords": [],
  "author": "CasjaysDev <git-admin@casjaysdev.com> (https://wherescovid.casjay.coffee)",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "jquery": "^3.4.1",
    "parcel": "^1.12.4",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "lite-server": "^2.5.4",
    "sass": "^1.26.2"
  }
}
