{
  "name": "wherescovid",
  "version": "1.0.0",
  "description": "Covid-19 cases",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "start": "parcel src/index.html",
    "build": "NODE_ENV=production parcel build src/index.html --no-source-maps --dist-dir public",
    "dev": "parcel src/index.html --dist-dir dist",
    "github": "NODE_ENV=production parcel build src/index.html --no-source-maps --dist-dir docs",
    "serve": "node server.js",
    "serve:build": "npm run build && npm run serve",
    "now": "vercel && vercel --prod"
  },
  "keywords": [],
  "author": "CasjaysDev <git-admin@casjaysdev.com> (https://wherescovid.casjay.coffee)",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^5.3.3",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "parcel": "^2.12.0",
    "eslint": "^8.57.0",
    "sass": "^1.77.8",
    "@parcel/transformer-sass": "^2.12.0"
  }
}
